{
  "name": "storemanager-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "test:coverage": "jest --collectCoverage",
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --hot --open",
    "build": "webpack --mode production --config ./webpack/webpack.config.js --open",
    "lint:fix": "eslint . --ignore-path .gitignore --fix",
    "heroku-prebuild": "npm i yarn; yarn install",
    "heroku-postbuild": "yarn build"
  },
  "repository": "git@github.com:armstrongsouljah/storemanager-frontend.git",
  "author": "Muhwezi Armstrong <armstrongsouljah@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.16.4",
    "less-loader": "^4.1.0",
    "path": "^0.12.7",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.1",
    "yarn": "^1.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "7",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "less": "^3.9.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
